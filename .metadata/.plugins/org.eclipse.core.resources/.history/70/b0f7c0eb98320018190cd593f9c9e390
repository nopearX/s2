package partie3;

public class Volaille {
	
	double poids;
	public int numID;
	
	Volaille(double poids, int Id){
		this.poids = poids;
		this.numID = Id;
	}
	
	void changePoids(double np) {
		poids=np;
	}
	
	public double prix() {
		return this.prix();
	}
	
	boolean assezGrosse() {
		return false;
	}
	
	public double getPoids() {
		return this.poids;
	}
}

